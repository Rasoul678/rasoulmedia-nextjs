{
  "name": "rasoulmedia-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate:dev": "npx dotenv -e .env.local -- prisma migrate dev",
    "db:push": "npx dotenv -e .env.local -- prisma db push",
    "migrate:reset": "npx dotenv -e .env.local -- prisma migrate reset",
    "db:seed": "npx dotenv -e .env.local -- prisma db seed",
    "prisma:generate": "npx dotenv -e .env.local -- prisma generate",
    "prisma:studio": "npx dotenv -e .env.local -- prisma studio",
    "production:build": "npx prisma generate && npx prisma migrate deploy && next build",
    "postgres:up": "docker-compose --env-file .env.local up",
    "postgres:down": "docker-compose --env-file .env.local down",
    "clean:docker": "docker system prune -af",
    "clean:volumes": "docker volume prune -f"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.0",
    "@formatjs/intl-localematcher": "^0.4.0",
    "@prisma/client": "^4.16.2",
    "@tanstack/react-query": "^4.29.25",
    "@tanstack/react-query-devtools": "^4.29.25",
    "@types/node": "20.3.3",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@vercel/postgres": "^0.4.0",
    "autoprefixer": "10.4.14",
    "bcrypt": "^5.1.0",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.7",
    "negotiator": "^0.6.3",
    "next": "13.4.7",
    "next-auth": "^4.22.1",
    "nextjs-toploader": "^1.4.2",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.1.6",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/negotiator": "^0.6.1",
    "dotenv-cli": "^7.2.1",
    "prisma": "^4.16.2"
  }
}
